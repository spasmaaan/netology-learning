[webservers]
%{for ws in webservers~}
${ws["name"]} ansible_host=${ws["network_interface"][0]["nat_ip_address"]} fqdn=${ws["fqdn"]}
%{endfor~}

[databases]
%{for dbs in dbservers~}
${dbs["name"]} ansible_host=${dbs["network_interface"][0]["nat_ip_address"]} fqdn=${dbs["fqdn"]}
%{endfor~}

[storage]
%{for st in storages~}
${st["name"]} ansible_host=${st["network_interface"][0]["nat_ip_address"]} fqdn=${st["fqdn"]}
%{endfor~}